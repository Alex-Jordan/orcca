                              }
                              if(!$ansHash->{isPreview}){push(@errors,"You need to provide more points")if$i&lt;3;}
                              return($score,@errors);
                            });
                            $change=Real($f{$later}-$f{$year});
                            Context("Percent");
                            Context()->flags->set(tolerance=>0.02);
                            $changepercent=Percent("$change %");
                            $cheval=$changepercent->cmp()->withPostFilter(AnswerHints($change=>'Do you mean to answer with a % sign?'));
                            $gr=init_graph(1950,-10,2020,50,axes=>[1960,0],grid=>[7,6],size=>[400,400]);
                            $gr->lb('reset');
                            for my$i(197..201){$gr->lb(new Label($i*10,-3,$i*10,'black','center','top'));
                                $gr->moveTo($i*10,-1);$gr->lineTo($i*10,1,'black',2);};
                            for my$i(1..4){$gr->lb(new Label(1960-2,$i*10,$i*10,'black','right','middle'));
                                $gr->moveTo(1960-1,$i*10);$gr->lineTo(1960+1,$i*10,'black',2);}
                            $gr->lb(new Label(2019,1,"year",'black','right','bottom'));
                            $gr->lb(new Label(1962,48,'%','black','left','middle'));
                            for my$i(@x){$gr->stamps(closed_circle($i,$f{$i},'blue'));};
                        </pg-code>
                    </setup>
                    <statement>
                        <p>This graph gives the percent of the US population that is foreign born in Census years from 1960 through 2010. (Source: <url href="http://www.pewhispanic.org/2015/09/28/chapter-5-u-s-foreign-born-population-trends/" />.)</p>
                        <p><image pg-name="$gr" /></p>
                        <p>What are the Cartesian coordinates for the point representing the year <var name="$year"/>? <var name="$point" width="10" /></p>
                        <p>Compared to in <var name="$year"/>, by <var name="$later"/> the percent of the US population that is foreign born rose an additional <var name="$changepercent" evaluator="$cheval" width="2" />.</p>
                        <p>List at least three ordered pairs in the graph. (Use commas to separate the ordered pairs.)</p>
                        <p><var name="$list" evaluator="$listeval" width="30" /></p>
                    </statement>
                </webwork>
