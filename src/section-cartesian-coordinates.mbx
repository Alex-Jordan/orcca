    <example xml:id="example-cartesian-points">
        <statement>
            <p>On paper, sketch a Cartesian coordinate system with units, and then plot the following points: <m>(3,2),(-5,-1),(0,-3),(4,0)</m>.</p>
        </statement>
        <solution>
            <sidebyside widths="46% 46%">
                <figure>
                    <image>
                        <description></description>
                        <latex-image-code>
                        <![CDATA[\begin{tikzpicture}
                            \begin{axis}[xmin=-5,xmax=5,
                                        ymin=-5,ymax=5,
                                        xtick={-4,-3,...,4},
                                        ytick={-4,-3,...,4}]
                                \addplot[primarycurve,->] coordinates {(0,0) (3,0) (3, 2)};
                                \addplot[primarycurve,->] coordinates {(0,0) (-2,0) (-2, -1)};
                                \addplot[primarycurve,->] coordinates {(0,0) (0,-3)};
                                \addplot[primarycurve,->] coordinates {(0,0) (4,0)};
                            \end{axis}
                        \end{tikzpicture}]]>
                        </latex-image-code>
                    </image>
                </figure>
                <figure>
                    <image>
                        <description></description>
                        <latex-image-code>
                        <![CDATA[\begin{tikzpicture}
                            \begin{axis}[xmin=-5,xmax=5,
                                        ymin=-5,ymax=5,
                                        xtick={-4,-3,...,4},
                                        ytick={-4,-3,...,4}]
                                \addplot[soliddot] coordinates {(3, 2)} node[above] {$(3,2)$};
                                \addplot[soliddot] coordinates {(-2, -1)} node[below] {$(-2,-1)$};
                                \addplot[soliddot] coordinates {(0, -3)} node[right] {$(0,-3)$};
                                \addplot[soliddot] coordinates {(4, 0)} node[above] {$(4,0)$};
                            \end{axis}
                        \end{tikzpicture}]]>
                        </latex-image-code>
                    </image>
                </figure>
            </sidebyside>
        </solution>
    </example>

