                            ytick={-8,-6,...,8},
                            minor ytick={-10,-9,...,10},
                            minor xtick={-10,-9,...,10},
                                ]
                                \addplot+[domain=-10:10,-,samples=100] {(x^3)/100-2*x+1};
                            \end{axis}
                        \end{tikzpicture}
                        ]]>
                    </latex-image>
