      <exercise>
 <!-- cubic -->
        <webwork>
            <setup>
            <pg-code>
                Context('Numeric');
                $s = Real(random(-3,3,1));
                $r = $s - random(1,3,1);
                $t = $s + random(2,3,1);
                $f = Formula("(x-$r)(x-$s)(x-$t)");
                @hts = map{abs($f->eval(x=>$_))}($r..$t);
                $amplitude = random(1,3,1);
                $c = $amplitude/max(@hts)*random(-1,1,2);
                $k = random(1,6-$amplitude,1)*random(-1,1,2);
                $f = $c*$f+$k;
                $gr = init_graph(-7,-7,7,7,
                   axes=>[0,0],
                   grid=>[14,14],
                   size=>[240,240]
                );
                $gr->lb('reset');
                for $j (-6..6){
                    if (Real($j)==0) {next;}
                    $gr->lb(new Label($j,0,$j,'black','center','top','large'));
                    $gr->lb(new Label(0,$j,$j,'black','right','middle','large'));
                };
                $gr->lb(new Label(7,0,'x','black','right','bottom','large'));
                $gr->lb(new Label(0,7,'y','black','left','top','large'));
                $start = $r;
                while (abs($f->eval(x=>$start-0.1))&lt;7 and $start &gt; -7) {$start = $start - 0.1};
                $stop = $t;
                while (abs($f->eval(x=>$stop+0.1))&lt;7 and $stop &lt; 7) {$stop = $stop + 0.1};
                add_functions($gr, "$f for x in &lt;$start,$stop> using color:blue and weight:1");
                $px = 14/240;
                $gr->moveTo($start+$px,$f->eval(x=>$start+$px));
                $gr->arrowTo($start,$f->eval(x=>$start),'blue',1);
                $gr->moveTo($stop-$px,$f->eval(x=>$stop-$px));
                $gr->arrowTo($stop,$f->eval(x=>$stop),'blue',1);
                $ans[0] = ($c > 0) ? Union("($r,$s)U($t,inf)") : Union("(-inf,$r)U($s,$t)");
                $ans[1] = ($c > 0) ? Union("(-inf,$r]U[$s,$t]") : Union("[$r,$s]U[$t,inf)");
            </pg-code>
            </setup>
            <statement>
              <p>
                A graph of <m>f</m> is given.
