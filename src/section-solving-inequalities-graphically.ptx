    <exercisegroup cols="3">
      <title>Solving Inequalities Graphically</title>
      <exercise>
 <!-- linear -->
        <webwork>
            <setup>
            <pg-code>
                $numerator = random(1,5,1);
                $denominator = random(1,5,1);
                $h = non_zero_random(-6,6,1);
                $k = non_zero_random(-6,6,1);
                $m = $numerator/$denominator*$h*$k/abs($h*$k);
                $k2 = ($k > 0) ? $k - $numerator : $k + $numerator;
                $h2 = ($k2-$k)/$m+$h;
                @k = num_sort($k,$k2);
                @h = num_sort($h,$h2);
                $f = Formula("$m(x - $h)+$k");
                $ans[0] = ($m > 0) ? Interval("($h,inf)") : Interval("(-inf,$h)");
                $ans[1] = ($m > 0) ? Interval("($h[0],$h[1]]") : Interval("[$h[0],$h[1])");
                $gr = init_graph(-7,-7,7,7,
                   axes=>[0,0],
                   grid=>[14,14],
                   size=>[240,240]
                );
                $gr->lb('reset');
                for $j (-6..6){
                    if ($j==0 or $j%2!=0) {next;}
                    $gr->lb(new Label($j,0,$j,'black','center','top','large'));
                    $gr->lb(new Label(0,$j,$j,'black','right','middle','large'));
                };
                $gr->lb(new Label(7,0,'x','black','right','bottom','large'));
                $gr->lb(new Label(0,7,'y','black','left','top','large'));
                @xvalues = num_sort(-7,7,(7-$k)/$m+$h,(-7-$k)/$m+$h);
                $gr->moveTo($h,$k);
                $gr->arrowTo($xvalues[1],$m*($xvalues[1]-$h)+$k,'blue',1);
                $gr->moveTo($h,$k);
                $gr->arrowTo($xvalues[2],$m*($xvalues[2]-$h)+$k,'blue',1);
            </pg-code>
            </setup>
            <statement>
              <p>
                A graph of <m>f</m> is given.
                Use the graph alone to solve the compound inequalities.
              </p>
              <sidebyside widths="40%">
                <image pg-name="$gr" />
              </sidebyside>
              <instruction>
                Write your solution sets in interval notation.
              </instruction>
              <p>
                <ol label="a.">
                  <li>
                    <p>
                      <m>f(x)\gt <var name="$k" /></m>
                    </p>
                    <p>
                      <var name="$ans[0]" width="10" />
                    </p>
                  </li>
                  <li>
                    <p>
                      <m><var name="$k[0]" /> \lt f(x) \le <var name="$k[1]" /></m>
                    </p>
                    <p>
                      <var name="$ans[1]" width="10" />
                    </p>
                  </li>
                </ol>
              </p>
            </statement>
        </webwork>
      </exercise>
