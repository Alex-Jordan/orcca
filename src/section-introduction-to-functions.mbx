						\node[ele,label=right:Yvonne] (a2) at (8,4) {};
						\node[ele,label=right:Georgia] (a3) at (8,3) {};
						\node[ele,label=right:Emma] (a4) at (8,2) {};
						\node[ele,label=right:Isabella] (a5) at (8,1) {};

						\node[scale=2] at (0,6.5) {Domain};
						\node[ele,label=left:tan] (b1) at (0,4) {};
						\node[ele,label=left:brown] (b2) at (0,3) {};
						\node[ele,label=left:green] (b3) at (0,2) {};
						\node[ele,label=left:white] (b4) at (0,1) {};

						\node[draw,thick,fit= (a5) (a4) (a3) (a2) (a1),minimum width=5cm] {} ;
						\node[draw,thick,fit= (b1) (b2) (b3) (b4),minimum width=5cm] {} ;
						\draw[->,thick,shorten <=2pt,shorten >=2pt] (b1) -- (a1);
						\draw[->,thick,shorten <=2pt,shorten >=2] (b2) -- (a2);
